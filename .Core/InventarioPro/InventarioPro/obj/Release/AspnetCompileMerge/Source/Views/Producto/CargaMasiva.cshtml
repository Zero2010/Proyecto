
@{
    ViewBag.Title = "CargaMasiva";
    Layout = "~/Views/Shared/_CargaMasiva.cshtml";
}

1 <p>@ViewBag.CountProducto</p>
2 <p>@ViewBag.CountProductoRpt</p>
3. <p>@TempData["CountProducto"]</p>
4. <p>@TempData["ListaProductoRpteCount"]</p>

5. <p>@Session["CountProducto"]</p>
6. <p>@Session["CountProductoRpt"]</p>

<div class="x_panel">
    <div class="x_title">
        <h2>Projects</h2>
        <ul class="nav navbar-right panel_toolbox">
            <li>
                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
            </li>
            <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                    <a class="dropdown-item" href="#">Settings 1</a>
                    <a class="dropdown-item" href="#">Settings 2</a>
                </div>
            </li>
            <li>
                <a class="close-link"><i class="fa fa-close"></i></a>
            </li>
        </ul>
        <div class="clearfix"></div>
    </div>
    <div class="x_content">

        <p>Simple table with project listing with progress and editing options</p>

        <!-- start project list -->
        <table class="table table-striped projects">
            <thead>
                <tr>
                    <th style="width: 1%">#</th>
                    <th style="width: 20%">Project Name</th>
                    <th>Project Progress</th>
                    <th>Status</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>#</td>
                    <td>
                        <a>Pesamakini Backend UI </a>@ViewBag.CountProducto
                        <br>
                        <small>Registros : @ViewBag.CountProducto</small>

                    </td>
                    <td class="project_progress">
                        <div class="progress progress_sm">
                            <div class="progress-bar bg-green" role="progressbar" data-transitiongoal="57" aria-valuenow="56" style="width: 57%;"></div>
                        </div>
                        <small>57% Complete</small>
                    </td>
                    <td>
                        <button type="button" class="btn btn-success btn-xs">Success</button>
                    </td>
                </tr>
                <tr>
                    <td>#</td>
                    <td>
                        <a>Pesamakini Backend UI</a>
                        <br>
                        <small>Registros : @ViewBag.CountProductoRpt</small>
                    </td>
                    <td class="project_progress">
                        <div class="progress progress_sm">
                            <div class="progress-bar bg-green" role="progressbar" data-transitiongoal="57" aria-valuenow="56" style="width: 57%;"></div>
                        </div>
                        <small>57% Complete</small>
                    </td>
                    <td>
                        <button type="button" class="btn btn-success btn-xs">Success</button>
                    </td>
                </tr>
            </tbody>
        </table>
        <!-- end project list -->

    </div>
</div>


@section scripts {
    <script>
        $(document).ready(function () {
            location.reload();
        });
    </script>

    }





